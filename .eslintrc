{
  "extends": "airbnb/base",
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "guard-for-in": 0,
    "func-names": 0,
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "arrow-body-style": [
      2,
      "as-needed"
    ],
    "no-mixed-operators": 0,
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "indent": [2, 2, {"SwitchCase": 1}],
    "consistent-return": 0
  },
  "env": {
    "browser": true
  }
}
